    "import numpy as np\n",
    "from litedl.utils import OneHotEncoder\n",
    "\n",
    "data = np.arange(10).reshape(-1, 1)\n",
    "data2 = np.arange(15, 1, -1).reshape(-1, 1)\n",
    "one = OneHotEncoder()\n",
    "one.fit(data)\n",
    "data = one.transform(data)\n",
    "data = one.transform(data2)\n",
    "\n",
    "print(data)"
